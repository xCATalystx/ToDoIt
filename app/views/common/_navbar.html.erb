<div>
  <% if Current.user %>
    <%= Current.user.email %>
    <%= button_to "Logout", logout_path, method: :delete %>
    
    <% if Current.user.admin? %>
    <button><%= link_to 'User List', admin_users_path %></button>
    <% end %>
  <% else %>
    <%= link_to "Sign Up", sign_up_path %>
    <%= link_to "Login", sign_in_path %>
  <% end %>

  
</div>